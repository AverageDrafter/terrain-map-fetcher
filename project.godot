config_version=5

[application]

config/name="TerrainMerge Tool"
run/main_scene="res://addons/terrain_map_fetcher/ui/map_fetcher_panel.tscn"
config/features=PackedStringArray("4.6", "Forward Plus")
config/icon="res://icon.svg"

[editor_plugins]

enabled=PackedStringArray("res://addons/terrain_3d/plugin.cfg", "res://addons/terrain_map_fetcher/plugin.cfg")

[physics]

3d/physics_engine="Jolt Physics"

[rendering]

rendering_device/driver.windows="d3d12"
